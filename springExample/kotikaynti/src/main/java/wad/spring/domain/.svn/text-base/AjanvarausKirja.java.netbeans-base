package wad.spring.domain;

import java.io.Serializable;
import java.util.List;
import javax.persistence.*;

/**
 *
 * @author kristiak
 */
@Entity
public class AjanvarausKirja implements Serializable {
    @Id
    @GeneratedValue(strategy = GenerationType.AUTO)
    private Long id;
    @OneToMany(mappedBy = "ajanvarauskirja", cascade={CascadeType.MERGE, CascadeType.PERSIST})
    private List<AjanvarausAika> ajat;

    
    public List<AjanvarausAika> getAjat() {
        return ajat;
    }

    public void setAjat(List<AjanvarausAika> ajat) {
        this.ajat = ajat;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }
    
}
